#!/usr/local/bin/expect
set timeout 20
set ip [lindex $argv 0]
set user [lindex $argv 1]
set password [lindex $argv 2]
set icmd [lindex $argv 3]

spawn ssh "$user\@$ip"
expect {
	"password:" {send "$password\r"}
	"yes/no" {
		send "yes\r"
		expect "password:"
		send "$password\r"
	}
}
expect "*]*"
send "$icmd\r";
#interact
expect "*]$*"
send "exit\r";
expect eof
#interact

